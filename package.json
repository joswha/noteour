{
  "name": "noteour",
  "displayName": "noteour",
  "description": "collecting notes",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:extension.collectNotes",
    "onCommand:extension.openAuditNotes"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.collectNotes",
        "title": "Collect Audit Notes"
      },
      {
        "command": "extension.openAuditNotes",
        "title": "Open Audit Notes"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "auditNotesExplorer",
          "title": "Audit Notes",
          "icon": "resources/audit-icon.svg"
        }
      ]
    },
    "views": {
      "auditNotesExplorer": [
        {
          "id": "auditNotesView",
          "name": "Audit Notes"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
